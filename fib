def fib(number,nth):
    list1 = []
    first = 1
    second = 1
    #print(second,end=',')
    list1.append(second)
    while(second <= number):
        #print(second,end=',')
        list1.append(second)
        first, second = second, (first+second)

    return list1[nth]

if __name__ == '__main__':

    number = int(input("Enter the number:"))
    nth = int(input("Enter the nth number"))
    print(fib(number,nth))
